{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "require-jsdoc": "off",
    "max-len": ["error", { "code": 100 }],
    "new-cap": [
      "error",
      {
        "capIsNewExceptions": [
          "Router", "DECIMAL", "ENUM"
        ]
      }
    ],
    "indent": [
      "error",
      2
    ]
  }
}